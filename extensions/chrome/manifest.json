{
  "name": "Timesuck",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Track the time you spend on websites",
  "icons": { "16": "images/icon16.png",
           "48": "images/icon48.png",
          "128": "images/icon128.png" },
  "background": {
    "scripts": ["js/timesuck.js"]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "Show Report"
  },
  "permissions": [
    "tabs",
    "windows",
    "unlimitedStorage"
  ]
}
